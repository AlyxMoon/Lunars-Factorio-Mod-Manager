{
  "name": "lunar-mod-manager",
  "version": "1.10.0",
  "description": "An unofficial mod manager for Factorio",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "standard": "standard",
    "standard:fix": "standard --fix",
    "test": "mocha test --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "clean": "rmdir /s /q dist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=all --out=dist --ignore=test --ignore=data/* --ignore=.babelrc --ignore=.gitignore --ignore=README.md",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=all --out=dist --ignore=test --ignore=data/* --ignore=.babelrc --ignore=.gitignore --ignore=README.md",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=all--out=dist --ignore=test --ignore=data/* --ignore=.babelrc --ignore=.gitignore --ignore=README.md",
    "package": "npm run package:win && npm run package:mac && npm run package:linux"
  },
  "keywords": [
    "Factorio",
    "mod",
    "manager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlyxMoon/Lunars-Factorio-Mod-Manager.git"
  },
  "bugs": {
    "url": "https://github.com/AlyxMoon/Lunars-Factorio-Mod-Manager/issues"
  },
  "homepage": "https://github.com/AlyxMoon/Lunars-Factorio-Mod-Manager",
  "author": "Alyx DeLunar",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.2.0",
    "mocha": "^3.2.0",
    "mocha-standard": "^1.0.0",
    "sinon": "^2.0.0",
    "standard": "^9.0.2"
  },
  "dependencies": {
    "jszip": "^3.1.1",
    "moment": "^2.14.1",
    "request": "^2.74.0"
  }
}
