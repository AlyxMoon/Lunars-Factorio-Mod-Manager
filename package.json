{
  "name": "lunar-mod-manager",
  "version": "1.11.1",
  "description": "An unofficial mod manager for Factorio",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "standard": "standard",
    "standard:fix": "standard --fix",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\" ",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "bundle": "webpack",
    "webpack-dev": "webpack-dev-server",
    "clean": "rmdir /s /q dist",
    "package:win": "electron-packager . --overwrite --prune=true --platform=win32 --arch=all --out=dist --icon=assets/icons/icon.ico --ignore=test --ignore=src --ignore=data/* --ignore=.gitignore --ignore=README.md",
    "full-package:win": "npm run test && npm run bundle && npm run package:win",
    "package:mac": "electron-packager . --overwrite --prune=true --platform=darwin --arch=all --out=dist --icon=assets/icons/icon.icns --ignore=test --ignore=src --ignore=data/* --ignore=.gitignore --ignore=README.md",
    "full-package:mac": "npm run test && npm run bundle && npm run package:mac",
    "package:linux": "electron-packager . --overwrite --prune=true --platform=linux --arch=all--out=dist --icon=assets/icons/icon.png --ignore=test --ignore=src --ignore=data/* --ignore=.gitignore --ignore=README.md",
    "full-package:linux": "npm run test && npm run bundle && npm run package:linux",
    "package:all": "npm run test && npm run bundle && npm run package:win && npm run package:mac && npm run package:linux"
  },
  "keywords": [
    "Factorio",
    "mod",
    "manager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlyxMoon/Lunars-Factorio-Mod-Manager.git"
  },
  "bugs": {
    "url": "https://github.com/AlyxMoon/Lunars-Factorio-Mod-Manager/issues"
  },
  "homepage": "https://github.com/AlyxMoon/Lunars-Factorio-Mod-Manager",
  "author": "Alyx DeLunar",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.27.3",
    "electron": "^1.6.2",
    "electron-packager": "^7.4.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "mocha-standard": "^1.0.0",
    "node-sass": "^4.5.1",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^6.0.3",
    "sinon": "^2.0.0",
    "standard": "^9.0.2",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "jszip": "^3.1.1",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "request": "^2.74.0"
  }
}
